  <h2 class="form-signin-heading text-center">Settings</h2>
  <div class="form-group">
    <label for="givenName" class="col-xs-4 control-label">Given Name</label>
    <div class="input-group col-xs-8">
      <input id="givenName" type="text" class="form-control" placeholder="Given Name" value="{{givenName}}" required="" autofocus="" disabled>
      <span class="input-group-btn">
        <button class="btn btn-default" type="button">Edit</button>
      </span>
    </div>
  </div>
</div>
<div class="form-group">
  <label for="familyName" class="col-xs-4 control-label">Family Name</label>
  <div class="input-group col-xs-8">
    <input id="familyName" type="text" class="form-control" placeholder="Family Name" required="" value="{{familyName}}" disabled>
    <span class="input-group-btn">
      <button class="btn btn-default" type="button">Edit</button>
    </span>
  </div>
</div>
</div>
<div class="form-group">
  <label for="email" class="col-xs-4 control-label">Email</label>
  <div class="col-xs-8">
    <input type="email" class="form-control" id="email" placeholder="Email" value="{{email}}">
  </div>
</div>


{{#each plans}}
<div class="panel panel-default">
  <div class="panel-heading">{{ this.title }}</div>
  <div class="panel-body">

    <div class="form-group">
      <label class="control-label col-xs-4" for="rung-{{ this.id }}">Level</label>
      <div class="col-xs-3">
        <select class="form-control" id="rung-{{ this.id }}">
          {{#each rungs}}
          <option {{#if this.selected}}selected="selected"{{/if}}>{{ this.id }}</option>
          {{/each}}
        </select>

      </div>
    </div>
  </div>
</div>
{{/each}}
