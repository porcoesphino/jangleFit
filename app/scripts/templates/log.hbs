<div class="panel-group" id="table-accordian">
  <div class="panel panel-success">

    <div class="panel-heading">Logs</div>

    {{#each sessions}}
    <div class="panel-heading">
      <a data-toggle="collapse" data-parent="#table-accordian" href="#{{ this.id }}">
        {{ this.timestamp }}
      </a>
    </div>
    <div id="{{ this.id }}" class="panel-collapse collapse">
      <div class="table-responsive">
        <table class="table table-striped table-bordered table-condensed">
          <thead>
            <tr>
              <th>Timestamp</th>
              <th>Activity</th>
              <th>Duration</th>
              <th>Count</th>
            </tr>
          </thead>
          <tbody>

            {{#each sets}}
            <tr>
              <td>{{ this.timestamp }}</td>
              <td>{{ this.activity }}</td>
              <td>{{ this.duration }}</td>
              <td>{{ this.amount }}</td>
              <td>{{ this.unit }}</td>
            </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
    </div>
    {{/each}}

  </div>
</div>
