<div class="table-responsive">
  <table class="table table-striped table-bordered table-condensed">
    <thead>
      <tr>
        {{#each headers}}
        <th>this</th>
        {{/each}}
      </tr>
    </thead>
    <tbody>
      {{#each rows}}
      <tr>
        {{#each cols}}
        <th>this</th>
        {{/each}}
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>
